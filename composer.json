{
  "name": "hughgrigg/the-api-strikes-back",
  "description": "Sample project around API consumption and data processing",
  "version": "1.0.0",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Hugh Grigg",
      "email": "hugh.grigg@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^7.0",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "johnkary/phpunit-speedtrap": "^3.0",
    "phpunit/phpunit-mock-objects": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "DeathStarApi\\": "src/",
      "DeathStarApi\\Tests\\": "tests/"
    }
  }
}
